{
  "name": "uno-merge",
  "version": "0.0.0",
  "packageManager": "pnpm@10.7.1",
  "description": "Utility function to efficiently merge UnoCSS classes without style conflicts",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/zhangmo8/uno-merge#readme",
  "bugs": {
    "url": "https://github.com/zhangmo8/uno-merge/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangmo8/uno-merge.git"
  },
  "author": "zhangmo8",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "@sxzz/eslint-config": "^6.1.1",
    "@sxzz/prettier-config": "^2.2.1",
    "@types/node": "^22.13.17",
    "bumpp": "^10.1.0",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "tsdown": "^0.7.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
